{
  "name": "sol-wallet-analyzer",
  "version": "1.0.0",
  "description": "Professional Solana wallet analyzer with real-time trader discovery capabilities",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "analyze": "ts-node src/index.ts",
    "discover": "ts-node wallet-discovery.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist temp node_modules/.cache .tsbuildinfo",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "solana",
    "wallet",
    "analyzer",
    "pnl",
    "trading",
    "discovery",
    "copytrading",
    "real-time",
    "defi",
    "blockchain"
  ],
  "author": {
    "name": "Anthony Stepvoy",
    "url": "https://github.com/anthonystepvoy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anthonystepvoy/sol-wallet-analyzer.git"
  },
  "bugs": {
    "url": "https://github.com/anthonystepvoy/sol-wallet-analyzer/issues"
  },
  "homepage": "https://github.com/anthonystepvoy/sol-wallet-analyzer#readme",
  "license": "MIT",
  "dependencies": {
    "@solana/web3.js": "^1.87.6",
    "@types/puppeteer": "^5.4.7",
    "@types/ws": "^8.18.1",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "puppeteer": "^24.12.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
